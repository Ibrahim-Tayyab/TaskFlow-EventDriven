{
  "version": 2,
  "buildCommand": "cd web-app && npm run build",
  "outputDirectory": "web-app/.next",
  "installCommand": "cd web-app && npm install",
  "framework": "nextjs",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "api"
  }
}